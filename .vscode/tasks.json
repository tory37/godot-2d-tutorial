{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Run Godot",
      "type": "shell",
      "command": "/Applications/Godot.app/Contents/MacOS/Godot",
      "args": [
        "--path",
        "${workspaceFolder}"
      ],
      "group": "build",
      "presentation": {
        "reveal": "always",
        "panel": "shared"
      },
      "problemMatcher": []
    }
  ]
}
